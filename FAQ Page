<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
<script>
$('.faq3').click(function(){
var = $answer = $(this).find('.answer');
if($answer.hasClass('open')) {
$answer.removeClass('open');
$answer.slideUp();

$(this).find('span').text('+');
} else {
$answer.addClass('open');
$answer.slideDown();

$(this).find('span').text('-');
}
});
});
</script>
